Module LearnArithmeticOperator01
	Sub Main()
		Dim a As Integer = 10
		Dim B As Integer = 20
		Dim ans As Integer
		
		ans = a + b
		Console.WriteLine(ans)
		
		ans = a - b
		Console.WriteLine(ans)
		
		ans = a * b
		Console.WriteLine(ans)
		
		Dim andD As Double = a / b
		Console.WriteLine(andD)
		
		ans = a \ b
		Console.WriteLine(ans)
		
		ans = a Mod b
		Console.WriteLine(ans)
	End Sub
End Module